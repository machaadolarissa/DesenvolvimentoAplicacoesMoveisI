<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/FirstFragment">

    <fragment
        android:id="@+id/FirstFragment"
        android:name="com.machado.trabalho2.FirstFragment"
        android:label=""
        tools:layout="@layout/fragment_first">
        <action
            android:id="@+id/action_firstFragment_to_secondFragment"
            app:destination="@id/SecondFragment" />
    </fragment>

    <fragment
        android:id="@+id/SecondFragment"
        android:name="com.machado.trabalho2.SecondFragment"
        tools:layout="@layout/fragment_second">

        <action
            android:id="@+id/action_SecondFragment_to_CommandHistoryFragment"
            app:destination="@id/CommandHistoryFragment" />

        <action
            android:id="@+id/action_SecondFragment_to_FirstFragment"
            app:destination="@id/FirstFragment" />

    </fragment>

    <fragment
        android:id="@+id/CommandHistoryFragment"
        android:name="com.machado.trabalho2.CommandHistoryFragment"
        android:label="Command History"
        tools:layout="@layout/fragment_command_history">

        <action
            android:id="@+id/action_History_to_SecondFragment"
            app:destination="@id/SecondFragment">
        </action>

        <action
            android:id="@+id/action_History_to_FirstFragment"
            app:destination="@id/FirstFragment" />
    </fragment>

</navigation>
